     */
    public static void main(String[] args) {
       int a = 5;
        Libro l1 = new Libro("El diario de greg", "Jeff", 12);
        Libro l2 = new Libro("Los secretos de YouTube", "TheGrefg", 30);
        Libro l3 = new Libro("El libro troll", "Rubius", 67);
        System.out.println(l1);
        System.out.println(l2);
        System.out.println(l3);
        l1.prestado();
        l1.setTitulo("Siddarth");
   }  
}


